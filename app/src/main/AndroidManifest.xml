<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.opendiary">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name="com.example.opendiary.ExpandPost"></activity>
        <activity android:name="com.example.opendiary.VideoCallOutgoing" />
        <activity android:name="com.example.opendiary.VideoCallinComing" />
        <activity android:name="com.example.opendiary.SettingsActivity" />
        <activity android:name="com.example.opendiary.NotificationActivity" />
        <activity android:name="com.example.opendiary.ShowLikedUser" />
        <activity android:name="com.example.opendiary.FollowerActivity" />
        <activity android:name="com.example.opendiary.UpdatePhoto" />
        <activity android:name="com.example.opendiary.ShowStory" />
        <activity android:name="com.example.opendiary.StoryActivity" />
        <activity android:name="com.example.opendiary.CommentsActivity" />
        <activity android:name="com.example.opendiary.SendImage" />
        <activity android:name="com.example.opendiary.MessageActivity" />
        <activity android:name="com.example.opendiary.ChatActivity" />
        <activity
            android:name="com.example.opendiary.IndividualPost"
            android:label="@string/title_activity_individual_post"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.opendiary.PostActivity" />
        <activity android:name="com.example.opendiary.AnswerActivity" />
        <activity android:name="com.example.opendiary.ReplyActivity" />
        <activity android:name="com.example.opendiary.YourQuestions" />
        <activity android:name="com.example.opendiary.Related" />
        <activity android:name="com.example.opendiary.AskActivity" />
        <activity
            android:name="com.example.opendiary.ImageActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.PrivacyActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.UpdateProfile"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.CreateProfile"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.RegisterActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.ShowUser"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.LoginActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name="com.example.opendiary.Splashscreen"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.example.opendiary.FirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>